<?xml version="1.0"?>
<Definitions xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <EntityComponents>

    <EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">

      <Id>
        <TypeId>Inventory</TypeId>
        <SubtypeId>IEC-Manipulation-Group</SubtypeId>
      </Id>
      <Description>

        [MES Manipulation Group]

        [ManipulationProfiles:MES-Manipulation-RivalAi]
        [ManipulationProfiles:IEC-Manipulation-InhibitorA]
        [ManipulationProfiles:IEC-Manipulation-InhibitorB]
        [ManipulationProfiles:IEC-Manipulation-DroneThrustHigh]
        [ManipulationProfiles:IEC-Manipulation-DroneEnergy]
        [ManipulationProfiles:IEC-Manipulation-BasicLoot]
        [ManipulationProfiles:IEC-Manipulation-MixedLoot]
        [ManipulationProfiles:IEC-Manipulation-AdvancedLoot]

      </Description>

    </EntityComponent>

    <EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">

      <Id>
        <TypeId>Inventory</TypeId>
        <SubtypeId>IEC-Manipulation-BasicLoot</SubtypeId>
      </Id>
      <Description>

        [MES Manipulation]

        [ManipulationRequiredCustomTags:Easy]

        [UseLootProfiles:true]
        [ClearExistingContainerTypes:true]
        [LootProfiles:MES-Loot-AntiInhibitor]
        [LootProfiles:MES-Loot-BasicContainers]
        [LootProfiles:MES-Loot-BasicContainersLarge]
        [LootProfiles:MES-Loot-Corpses]
        [LootProfiles:MES-Loot-Freight]
        [LootProfiles:MES-Loot-Lockers]
        [LootProfiles:MES-Loot-WeaponRacks]

      </Description>

    </EntityComponent>

    <EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">

      <Id>
        <TypeId>Inventory</TypeId>
        <SubtypeId>IEC-Manipulation-MixedLoot</SubtypeId>
      </Id>
      <Description>

        [MES Manipulation]

        [ManipulationRequiredCustomTags:Medium]

        [UseLootProfiles:true]
        [ClearExistingContainerTypes:true]
        [LootProfiles:MES-Loot-AntiInhibitor]
        [LootProfiles:MES-Loot-MixedContainers]
        [LootProfiles:MES-Loot-MixedContainersLarge]
        [LootProfiles:MES-Loot-Corpses]
        [LootProfiles:MES-Loot-Freight]
        [LootProfiles:MES-Loot-Lockers]
        [LootProfiles:MES-Loot-WeaponRacks]

      </Description>

    </EntityComponent>

    <EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">

      <Id>
        <TypeId>Inventory</TypeId>
        <SubtypeId>IEC-Manipulation-AdvancedLoot</SubtypeId>
      </Id>
      <Description>

        [MES Manipulation]

        [ManipulationRequiredCustomTags:Hard]

        [UseLootProfiles:true]
        [ClearExistingContainerTypes:true]
        [LootProfiles:MES-Loot-AdvancedContainers]
        [LootProfiles:MES-Loot-AdvancedContainersLarge]
        [LootProfiles:MES-Loot-AntiInhibitor]
        [LootProfiles:MES-Loot-Corpses]
        [LootProfiles:MES-Loot-Freight]
        [LootProfiles:MES-Loot-Lockers]
        [LootProfiles:MES-Loot-WeaponRacks]

      </Description>

    </EntityComponent>

    <EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">

      <Id>
        <TypeId>Inventory</TypeId>
        <SubtypeId>IEC-Manipulation-InhibitorA</SubtypeId>
      </Id>
      <Description>

        [MES Manipulation]

        [ManipulationAllowedPrefabIndexes:0]

        [ReplaceArmorBlocksWithModules:true]
        [ModulesForArmorReplacement:MyObjectBuilder_RadioAntenna/MES-Suppressor-Energy-Small]
        [ModulesForArmorReplacement:MyObjectBuilder_RadioAntenna/MES-Suppressor-Energy-Large]

      </Description>

    </EntityComponent>

    <EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">

      <Id>
        <TypeId>Inventory</TypeId>
        <SubtypeId>IEC-Manipulation-InhibitorB</SubtypeId>
      </Id>
      <Description>

        [MES Manipulation]
        
        [ManipulationRequiredCustomTags:Station]
        [ManipulationAllowedPrefabIndexes:0]

        [ReplaceArmorBlocksWithModules:true]
        [ModulesForArmorReplacement:MyObjectBuilder_RadioAntenna/MES-Suppressor-Drill-Large]

      </Description>

    </EntityComponent>

    <EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">

      <Id>
        <TypeId>Inventory</TypeId>
        <SubtypeId>IEC-Manipulation-Propulsion</SubtypeId>
      </Id>
      <Description>

        [MES Manipulation]

        [SetNpcGyroscopeMultiplier:true]
        [NpcGyroscopeMultiplier:2.5]

      </Description>

    </EntityComponent>

    <EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">

      <Id>
        <TypeId>Inventory</TypeId>
        <SubtypeId>IEC-Manipulation-DroneThrust</SubtypeId>
      </Id>
      <Description>

        [MES Manipulation]

        [UseBlockReplacerProfile:true]
        [IgnoreGlobalBlockReplacer:true]
        [BlockReplacerProfileNames:MES-NpcThrusters-Hydro]

        [ConfigureSpecialNpcThrusters:true]
        [NpcHydroThrustPowerMultiply:0.0001]
        [NpcHydroThrustForceMultiply:1]

      </Description>

    </EntityComponent>

    <EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">

      <Id>
        <TypeId>Inventory</TypeId>
        <SubtypeId>IEC-Manipulation-DroneThrustHigh</SubtypeId>
      </Id>
      <Description>

        [MES Manipulation]

        [ManipulationRestrictedPrefabIndexes:0]

        [UseBlockReplacerProfile:true]
        [IgnoreGlobalBlockReplacer:true]
        [BlockReplacerProfileNames:MES-NpcThrusters-Hydro]

        [ConfigureSpecialNpcThrusters:true]
        [NpcHydroThrustPowerMultiply:0.0001]
        [NpcHydroThrustForceMultiply:2]

      </Description>

    </EntityComponent>

    <EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">

      <Id>
        <TypeId>Inventory</TypeId>
        <SubtypeId>IEC-Manipulation-DroneEnergy</SubtypeId>
      </Id>
      <Description>

        [MES Manipulation]

        [ManipulationRestrictedPrefabIndexes:0]

        [UseBlockReplacerProfile:true]
        [IgnoreGlobalBlockReplacer:true]
        [BlockReplacerProfileNames:MES-Energy-SmallBatteryToNpcBattery]

      </Description>

    </EntityComponent>

    <EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">

      <Id>
        <TypeId>Inventory</TypeId>
        <SubtypeId>IEC-Manipulation-RandomizationLimits</SubtypeId>
      </Id>
      <Description>

        [MES Manipulation]

        [ManipulationRestrictedPrefabIndexes:0]

        [WeaponRandomizerTargetBlacklist:MyObjectBuilder_LargeGatlingTurret/SmallGatlingTurret]

      </Description>

    </EntityComponent>

    

  </EntityComponents>
</Definitions>